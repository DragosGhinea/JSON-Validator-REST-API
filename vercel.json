{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "hooks": {
    "build": {
      "before": {
        "npm": {
          "ci": false,
          "arguments": ["run", "build"]
        }
      }
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist-server/app.js"
    }
  ]
}
